<Window x:Class="SquirrelTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SquirrelTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="20">
        <StackPanel>
            <TextBlock HorizontalAlignment="Center" FontSize="40">
            <Bold>HELLO WORLD!</Bold>
            </TextBlock>
            <TextBlock Name="GreetingMessage" HorizontalAlignment="Center"/>
        </StackPanel>
        <TextBlock Name="ReleaseMessage" HorizontalAlignment="Right" VerticalAlignment="Bottom"/>
    </Grid>
</Window>
